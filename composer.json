{
    "name": "acme/user-account-ext",
    "type": "typo3-cms-extension",
    "description": "A TYPO3 extension which facilitates user registration and associated functionalities incorporating TYPO3's core fe_login extension.",
    "authors": [
        {
            "name": "Anu Bhuvanendran Nair",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "ACME\\UserAccountExt\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ACME\\UserAccountExt\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/user-account-ext": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "user_account_ext"
        }
    }
}